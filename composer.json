{
    "name": "martyn911/yii2-easy-start",
    "description": "Yii 2 Advanced Easy Start",
    "keywords": ["yii2", "easy start", "framework", "advanced", "project template"],
    "homepage": "https://github.com/martyn911/yii2-easy-start",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Martynenko",
            "email": "martyn911@i.ua",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Martyn911/yii2-easy-start/issues/new"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "ext-intl": "*",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "vlucas/phpdotenv": "^2.4",
        "dektrium/yii2-user": "^0.9.12",
        "dektrium/yii2-rbac": "^1.0.0-alpha@dev",
        "bower-asset/admin-lte": "^2.3",
        "bower-asset/font-awesome": "^4.7",
        "yiisoft/yii2-jui": "^2.0",
        "bower-asset/html5shiv": "^3.7",
        "bower-asset/jquery-slimscroll": "^1.3",
        "codemix/yii2-localeurls": "^1.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
